{
  "files.exclude": {
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.map": {
      "when": "$(basename).map"
    },
    "**/.git": true
  },
  "editor.rulers": [
    100
  ],
  "workbench.iconTheme": "vscode-icons",
  "search.exclude": {
    "**/.venv/": true,
    "**/venv/": true,
    "**/dist/": true,
    "**/.mypy_cache/": true
  },
  "search.useIgnoreFiles": false,
  "editor.renderWhitespace": "all",
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "editor.formatOnPaste": true,
  "editor.minimap.enabled": false,
  "explorer.confirmDelete": false,
  "files.associations": {
    "*.avsc": "json"
  },
  "explorer.confirmDragAndDrop": false,
  "git.confirmSync": false,
  "vsdocker.imageUser": "docker.io/melkerman",
  "docker.defaultRegistryPath": "melkerman",
  "git.suggestSmartCommit": false,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.suggestSelection": "first",
  "python.languageServer": "Pylance",
  "python.analysis.typeCheckingMode": "basic",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.editor.revealIfOpen": true,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[elixir]": {
    "editor.formatOnSave": true
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      10
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "vsicons.dontShowNewVersionMessage": true,
  "editor.tabCompletion": "on",
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.autolinks": [
    {
      "url": "https://dev.azure.com/BusinessVenture/Creator%20Studio/_boards/board/t/Station%20-%20Fulfill%20and%20Support/Stories/?workitem=<num>",
      "prefix": "TICKET-"
    }
  ],
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "charliermarsh.ruff",
  },
  "elixirLS.enableTestLenses": true,
  "elixirLS.suggestSpecs": false,
  "emmet.includeLanguages": {
    "phoenix-heex": "html",
    "html-eex": "html"
  },
"git.openRepositoryInParentFolders": "always",
"vs-kubernetes": {
  "vscode-kubernetes.minikube-path-linux": "/home/viktor/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube"
},
"github.copilot.editor.enableAutoCompletions": true,
"[html]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
},
"[typescript]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
},
"editor.multiCursorLimit": 15000,
"circleci.hostUrl": ""
}
