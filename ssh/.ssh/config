# HM github account
Host github.com-hm
    AddKeysToAgent yes
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_ed25519_hm

Host github.com
 AddKeysToAgent yes
 HostName github.com
 User git
 IdentityFile ~/.ssh/id_rsa

